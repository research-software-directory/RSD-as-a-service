{
  "name": "rsd-frontend",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:wp": "next dev",
    "dev:docker": "NODE_ENV=docker next dev  # backup: NODE_ENV=docker NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:coveragememlimit": "jest --coverage --workerIdleMemoryLimit=0.1 --maxWorkers=2",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage",
    "test:inspect": "node --inspect-brk --expose-gc ./node_modules/.bin/jest --logHeapUsage",
    "test:memlimit": "jest --workerIdleMemoryLimit=0.1 --maxWorkers=2",
    "knip:files": "knip --include files",
    "knip:types": "knip --include types",
    "knip:exports": "knip --include exports",
    "knip": "knip"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "7.1.0",
    "@mui/material": "7.1.0",
    "@tailwindcss/typography": "0.5.16",
    "aos": "2.3.4",
    "cookie": "1.0.2",
    "d3": "7.9.0",
    "dompurify": "3.2.5",
    "jsonwebtoken": "9.0.2",
    "next": "15.3.2",
    "nprogress": "0.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.3",
    "react-markdown": "10.1.0",
    "rehype-external-links": "3.0.0",
    "remark-breaks": "4.0.0",
    "remark-gfm": "4.0.1",
    "sharp": "0.34.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/postcss": "4.1.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/aos": "3.0.7",
    "@types/d3": "7.4.3",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/node": "22.15.18",
    "@types/nprogress": "0.2.3",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.56.0",
    "postcss": "8.5.3",
    "tailwindcss": "4.1.7",
    "typescript": "5.8.3",
    "whatwg-fetch": "3.6.20"
  }
}
